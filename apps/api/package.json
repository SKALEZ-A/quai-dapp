{
  "name": "api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "nodemon --watch src --exec ts-node src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "test": "echo \"(api) no tests yet\"",
    "prisma:generate": "prisma generate --schema=./prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=./prisma/schema.prisma",
    "indexer": "ts-node src/workers/indexer.ts"
  },
  "dependencies": {
    "@apollo/server": "^5.0.0",
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "graphql": "^16.11.0",
    "pg": "^8.16.3",
    "pino": "^9.3.2",
    "viem": "^2.37.5",
    "web3.storage": "^4.5.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.5.5",
    "eslint": "^9.9.0",
    "nodemon": "^3.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  }
}
